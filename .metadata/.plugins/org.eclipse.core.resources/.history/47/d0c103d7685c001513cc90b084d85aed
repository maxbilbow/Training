package fjwa;

public enum TableData {
	contact_information("contact_information",""
			+ "id unique int PRIMARY KEY,"
			+ "name varchar(255),"
			+ "email varchar(255)"
			);
	private String name;
	
	private TableData(String name) {
		this.name = name;
	}
	
	public String toString() {
		return name;
	}
	
	public void create() {
		CREATE TABLE contact_information (
				id unique int PRIMARY KEY,
				name varchar(255),
				email varchar(255)
				);
	}
}
