package fjwa.repository;

import java.util.Collection;

import fjwa.RMXException;
import fjwa.model.Bomb;



public interface EntityRepository<E> {

	Collection<E> loadAll() throws Exception;

	E save(E entity) throws Exception;

	E remove(E expired) throws Exception;

//	void synchronize(Collection<E> entities) throws RMXException;

	E synchronize(E entity) throws Exception;
}
