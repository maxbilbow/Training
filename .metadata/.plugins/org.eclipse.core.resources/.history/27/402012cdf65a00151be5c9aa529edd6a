
public class Threader extends Thread {

	
	
	public static void main(String[] args) {
		Threader t = new Threader();
		t.start();
		for (int i = 0; i<100; ++i);
			System.out.println((char) i);
	}

	@Override
	public void run() {
		
		for (int i = 0; i < 100; ++i) {
			System.out.println(i);
		}
		super.run();
	}

}
