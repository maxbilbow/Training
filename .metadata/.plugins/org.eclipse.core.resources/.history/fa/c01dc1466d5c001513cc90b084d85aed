package fjwa;

import java.sql.SQLException;

public interface IWebInterface {
	void openNewConnection() throws SQLException, ClassNotFoundException;
	void closeConnection();
	
	void setContext(TableData table) throws FJWAException;
	void clearContext();
	
	void setValueForKey(String key, Object value);
	void getValue(String key);
	boolean isConnected();
	
	default static WebInterface newConnectedInterface() {
		IWebInterface = new IWebInterface();
		return 
	}
	
}
